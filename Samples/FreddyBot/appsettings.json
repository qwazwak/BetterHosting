{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient.Default": "Warning"
    },
    "Console": {
      //"FormatterName": "simple",
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "servicesStartConcurrently": true,
  "servicesStopConcurrently": true,
  "AllowedHosts": "*",
  "ApiNinjaToken": null,

  "DiscordConfiguration": {
    "Token": "your token here",
    "UseRelativeRatelimit": true,
    "LogTimestampFormat": "yyyy-MM-dd HH:mm:ss zzz",
    "LargeThreshold": 250,
    "ReconnectIndefinitely": false,
    "AutoReconnect": true,
    "ShardId": 0,
    "ShardCount": 1,
    "GatewayCompressionLevel": "Stream",
    "MessageCacheSize": 1024,
    "HttpTimeout": "0:00:30",
    "AlwaysCacheMembers": true,
    //DiscordIntents Intents: DiscordIntents.AllUnprivileged,
    "LogUnknownEvents": true,
    "LogUnknownAuditlogs": true
  },
  "DefaultDiscordActivity": {
    "Name": "Vibing",
    "StreamUrl": null,
    "ActivityType": "Custom"
  }
}
